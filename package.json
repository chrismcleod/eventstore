{
  "name": "rx-eventstore-client",
  "version": "0.0.1-alpha.3",
  "description": "EventStore client using rxjs streams",
  "main": "index.js",
  "repository": "https://github.com/chrismcleod/rx-eventstore-client",
  "author": "Chris McLeod",
  "license": "MIT",
  "scripts": {
    "test": "TS_NODE_PROJECT=./test mocha",
    "test:watch": "TS_NODE_PROJECT=./test mocha --watch",
    "test:coverage": "TS_NODE_PROJECT=./test nyc mocha",
    "test:coverage!": "TS_NODE_PROJECT=./test nyc mocha && open ./coverage/index.html",
    "test:report": "cc-test-reporter before-build && nyc report --reporter=lcov && cc-test-reporter after-build --id e3be908ac31a800c338a768e78dc309484e695fb307cc2a523a06b5721886647 --exit-code $?",
    "g:buffers": "node ./scripts/buffers.js",
    "example": "node dist/example.js"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/chai-as-promised": "^0.0.31",
    "@types/lodash": "^4.14.71",
    "@types/long": "^3.0.31",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.17",
    "@types/ramda": "^0.24.5",
    "@types/sinon": "^2.3.3",
    "@types/sinon-chai": "^2.7.28",
    "@types/uuid": "^3.4.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "nyc": "^11.1.0",
    "rxjs-marbles": "^1.1.0",
    "sinon": "^2.3.8",
    "sinon-chai": "^2.12.0",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "esproto": "0.0.1-alpha.6",
    "ramda": "^0.24.1",
    "rxjs": "^5.4.2",
    "uuid": "^3.1.0",
    "uuid-parse": "^1.0.0"
  }
}